<h3>What task should created for <%= Project.all[0].title %>?</h3>

<%= form_with model: @task, local: true do |t| %>
    <%= t.label :Objective%><br>  
    <%= t.text_area :objective, size: "60x5" %> <br> 
    <%= t.label :Complete%><br>   
    <%= t.text_field :complete%><br>
    <%= t.submit "Create Project"%>      
<% end %>
      
      